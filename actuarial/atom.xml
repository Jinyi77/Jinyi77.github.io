---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Jinyi's Blog</title>
 <link href="http://jinyi77.github.io/blog/atom.xml" rel="self"/>
 <link href="http://jinyi77.github.io/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://jinyi77.github.io/blog</id>
 <author>
   <name>Jinyi Liu</name>
   <email>Jinyi77@gmail.com</email>
 </author>

 {% for post in site.actuarialposts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://jinyi77.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://jinyi77.github.io/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
